/ {
  combos {
    compatible = "zmk,combos";
    combo_caps {
        timeout-ms = <50>;
        key-positions = <16 38>;
        bindings = <&u_caps_word>;
    };
  };
  behaviors {
    u_mt_x: u_mt_x {
      compatible = "zmk,behavior-hold-tap";
      label = "u_mt_x";
      #binding-cells = <2>;
      tapping_term_ms = <U_TAPPING_X_TERM>;
      flavor = "tap-preferred";
      bindings = <&kp>, <&kp>;
    };
  };
};
